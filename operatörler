
//Atama

int x = 3;
int y = 3;
y = y+2;
y += 2;
y /= 1;
x *= 2;

//Mantıksal

bool isSuccess = true;
bool isComleted = false;

if(isSuccess && isComleted){
    Console.WriteLine("Perfect");
}

if(isSuccess || isComleted){
    Console.WriteLine("Great");
}

if(isSuccess && !isComleted){
    Console.WriteLine("Fİne");
}

